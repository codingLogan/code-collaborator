{
  "name": "collaborator",
  "displayName": "Collaborator",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "collaborator.helloWorld",
        "title": "Collaborator | Hello World"
      },
      {
        "command": "collaborator.time",
        "title": "Collaborator | Time"
      },
      {
        "command": "collaborator.refresh",
        "title": "Collaborator | Refresh",
        "icon": {
          "light": "src/resources/light/refresh.svg",
          "dark": "src/resources/dark/refresh.svg"
        }
      }
    ],
    "configuration": [
      {
        "title": "Collaborator",
        "properties": {
          "collaborator.gitlab-access-token": {
            "type": "string",
            "default": "<tokenvalue>",
            "description": "Personal Access token for your GitLab user"
          },
          "collaborator.gitlab-domain": {
            "type": "string",
            "default": "gitlab.com",
            "description": "GitLab Domain, ex: gitlab.com"
          }
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "collaborator.refresh",
          "when": "view == collaborator",
          "group": "navigation"
        }
      ]
    },
    "views": {
      "scm": [
        {
          "id": "collaborator",
          "name": "Collaborator"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.81.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
